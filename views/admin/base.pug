  body.h-100.w-100
    header.container-fluid.sticky-top.bg-primary
      #nav-container.container
        nav.navbar.navbar-expand-lg.navbar-dark.bg-primary
          a.navbar-brand(href='#') Admin
          button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarColor01', aria-controls='navbarColor01', aria-expanded='false', aria-label='Toggle navigation')
            span.navbar-toggler-icon
          #navbarColor01.collapse.navbar-collapse
            ul.navbar-nav.mr-auto.auth-protected
              li.nav-item
                a.nav-link(href='/admin')
                  | Dashboard 
                  span.sr-only (current)
              li.nav-item
                a.nav-link(href='/admin/content') Content
              li.nav-item
                a.nav-link(href='/admin/users') Users
              li.nav-item
                a.nav-link(href='/admin/settings') Settings
            ul.navbar-nav.ml-auto.auth-protected
              li.nav-item#logout
                a.nav-link#logout(href="#") Logout
    .container.h-100.w-100#container

